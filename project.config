[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = framework for emulating devices in userspace
[access "refs/*"]
	owner = group user/Thanos (tmakatos)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Thanos (tmakatos)
	label-Verified = -1..+1 group user/Thanos (tmakatos)
	push = group user/Thanos (tmakatos)
	read = group user/Thanos (tmakatos)
	rebase = group user/Thanos (tmakatos)
	removeReviewer = group user/Thanos (tmakatos)
	submit = group user/Thanos (tmakatos)
[access "refs/heads/*"]
	create = group user/Thanos (tmakatos)
	push = +force group user/Thanos (tmakatos)
	pushMerge = group user/Thanos (tmakatos)
	read = group user/Thanos (tmakatos)
[access "refs/tags/*"]
	push = group user/Thanos (tmakatos)
	push = +force group user/Thanos (tmakatos)
